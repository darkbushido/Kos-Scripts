function rendezvous {
  until p["T"]["Target"]:distance < 250 {
    rndz["cancel"](p["T"]["Target"]).
    if p["T"]["Target"]:distance > 1000
      rndz["approach"](p["T"]["Target"], 30).
    else
      rndz["approach"](p["T"]["Target"], 10).
    rndz["await_nearest"](p["T"]["Target"], 250).
  }
  rndz["cancel"](p["T"]["Target"]).
  next().
}
